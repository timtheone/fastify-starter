{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
  